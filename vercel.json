{
    "builds": [
        {
            "src": "aikido/wsgi.py",
            "use": "@vercel/python",
            "functions": {
                "api": {
                    "runtime": "python3.8"
                }
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "elevate/wsgi.py"
        }
    ]
}